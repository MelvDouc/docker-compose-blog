{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "exports": "./",
  "description": "A sample full-stack application to practice using Docker Compose.",
  "scripts": {
    "start": "pnpm compose --env-file database/.env.prod up --build",
    "build": "pnpm compose --env-file database/.env.prod build",
    "stop": "pnpm compose down",
    "compose": "docker compose -f docker-compose.prod.yml",
    "dev:start": "pnpm dev:compose --env-file database/.env.dev up --build",
    "dev:build": "pnpm dev:compose --env-file database/.env.dev build",
    "dev:stop": "pnpm dev:compose down",
    "dev:compose": "docker compose -f docker-compose.dev.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MelvDouc/docker-compose-blog.git"
  },
