{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "exports": "./",
  "description": "A sample full-stack application to practice using Docker Compose.",
  "scripts": {
    "start": "docker compose -f docker-compose.prod.yml --env-file database/.env.prod up --build",
    "build": "docker compose -f docker-compose.prod.yml --env-file database/.env.prod build",
    "up": "docker compose -f docker-compose.prod.yml --env-file database/.env.prod up",
    "down": "docker compose -f docker-compose.prod.yml down",
    "dev:start": "pnpm run dev:up --build",
    "dev:up": "docker compose -f docker-compose.dev.yml --env-file database/.env.dev up",
    "dev:down": "docker compose -f docker-compose.dev.yml down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MelvDouc/docker-compose-blog.git"
  },
